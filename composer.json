{
    "name": "wikiworldorder/survloop",
    "description": "SurvLoop is a Laravel-based engine for designing a database and creating a mobile-friendly user interface to fill it.",
    "keywords": [
    	"SurvLoop", 
    	"survey", 
    	"database designer", 
    	"user experience", 
    	"forms", 
    	"mobile friendly", 
    	"mobile first", 
    	"branching tree",
    	"nodes",
    	"content management system",
    	"open source",
    	"Laravel"
    ],
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Morgan Lesko",
            "homepage": "https://github.com/wikiworldorder/survloop",
            "email": "wikiworldorder@protonmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "doctrine/dbal": "^2.9.2", 
        "guzzlehttp/guzzle": "^6.3.3",
        "matthiasmullie/minify": "^1.3.61",
        "twbs/bootstrap": "^4.3.1",
        "components/jquery": "^3.3.1",
        "components/jqueryui": "^1.12.1",
        "forkawesome/fork-awesome": "^1.1.5",
        "nnnick/chartjs": "^2.7.3",
        "plotly/plotly.js": "^1.44.4",
        "summernote/summernote": "v0.8.8",
        "wikiworldorder/survloop-libraries": "^0.1.2"
    },
    "autoload": {
        "psr-4": {
            "SurvLoop\\": "src/",
            "SurvLoop\\Models\\": "storage/app/models/survloop",
            "MatthiasMullie\\Minify\\": "vendor/matthiasmullie/minify/src/",
            "MatthiasMullie\\PathConverter\\": "vendor/matthiasmullie/path-converter/src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "WikiWorldOrder\\SurvLoop\\SurvLoopServiceProvider"
            ],
            "aliases": {
                "SurvLoop": "WikiWorldOrder\\SurvLoop\\SurvLoopFacade"
            }
        }
    }
}
