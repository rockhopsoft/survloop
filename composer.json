{
    "name": "rockhopsoft/survloop",
    "description": "SurvLoop is a Laravel-based engine for designing a database and creating a mobile-friendly user interface to fill it.",
    "keywords": [
    	"SurvLoop", 
    	"survey", 
    	"database designer", 
    	"user experience", 
    	"forms", 
    	"mobile friendly", 
    	"mobile first", 
    	"branching tree",
    	"nodes",
    	"content management system",
    	"open source",
    	"Laravel"
    ],
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Morgan Lesko",
            "homepage": "https://github.com/rockhopsoft/survloop",
            "email": "rockhoppers@runbox.com",
            "role": "Developer"
        }
    ],
    "require": {
        "doctrine/dbal": "^2.10.0", 
        "guzzlehttp/guzzle": "^6.4.1",
        "matthiasmullie/minify": "^1.3.61",
        "twbs/bootstrap": "^4.3.1",
        "components/jquery": "^3.3.1",
        "components/jqueryui": "^1.12.1",
        "forkawesome/fork-awesome": "^1.1.5",
        "nnnick/chartjs": "^2.9.1",
        "plotly/plotly.js": "^1.51.0",
        "summernote/summernote": "v0.8.8",
        "rockhopsoft/survloop-libraries": "^0.1.5"
    },
    "autoload": {
        "psr-4": {
            "SurvLoop\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SurvLoop\\SurvLoopServiceProvider"
            ],
            "aliases": {
                "SurvLoop": "SurvLoop\\SurvLoopFacade"
            }
        }
    }
}
